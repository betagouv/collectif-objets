- if commune.inactive?
  %h2.co-text--blue
    = t("communes.objets.index.n_objets_a_recenser", count:)
  = render "recensements/etapes"
- elsif commune.can_complete?
  %h2.co-text--blue
    = t("communes.objets.index.tous_les_objets_recenses")
  = render "communes/objets/complete_callout", dossier:, commune:
- elsif commune.started?
  %h2.co-text--blue
    = t("communes.objets.index..n_objets_restants_a_recenser", count: commune.objets.without_completed_recensements.count)
  = render "recensements/etapes"
- elsif commune.completed? && dossier.submitted?
  - if commune.messages.from_conservateur.blank?
    = render "communes/objets/submitted_callout", commune:
- elsif commune.completed? && dossier.accepted?
  = render "communes/objets/accepted_callout", dossier:
